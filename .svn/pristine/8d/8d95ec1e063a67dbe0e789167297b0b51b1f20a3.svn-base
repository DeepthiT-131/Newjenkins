        <div class="category-header">
            <div class="row">
                <div class="col-sm-10 col-6">
                    <h5 class="cateogory-title">Category</h5>
                </div>
               
            </div>
        </div>
    </div>
  </div>
  
  <div class="box-shadow" *ngIf="categoryviewdetails?.length != 0">
    <div class="whitebground"> 
        <div class="container">
            <div class="row">
                <div class="col-sm-3 col-12 mt-3 posi" *ngFor="let item of categoryviewdetails">
                    <div class="cat-padding">
                        <a (click)="ShowForm(item.CategoryCode)"> {{item.CategoryName}} </a>
                    </div>
                  
                </div>
            </div>
        </div>
    </div>
  
    </div>
  
  
  
    
  
     
    